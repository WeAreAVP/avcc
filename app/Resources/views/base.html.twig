<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
        <!-- Apple devices fullscreen -->
        <meta name="apple-mobile-web-app-capable" content="yes">
        <!-- Apple devices fullscreen -->
        <meta names="apple-mobile-web-app-status-bar-style" content="black-translucent">
        <title>{% block title %}AVCC - WeAreAVP{% endblock %}</title>
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.png') }}" />
        {% block stylesheets %}
            <link href="{{ asset('css/bootstrap.min.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('css/login-style.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('css/modal.css') }}" type="text/css" rel="stylesheet" />
        {% endblock %}
    </head>
    <body class="theme-gray login">
        {% if qa_instance %}
            <div class="qa-instance">
                <h2 class="qa_heading">QA</h2>
            </div>
        {% endif %}
        <div class="wrapper">
            <h1>
                <a href="/">
                    {#				<img src="" alt="" class="retina-ready" width="59" height="49">#}
                    AVCC</a>
            </h1>
        {% block body %}{% endblock %}
    </div>
    

    {% block javascripts %}
        <script src="{{ asset('js/jquery.min.js') }}"></script>
        <script src="{{ asset('js/modal.js') }}"></script>
        {% if ga_tracking %}
            <script>
                (function (i, s, o, g, r, a, m) {
                    i['GoogleAnalyticsObject'] = r;
                    i[r] = i[r] || function () {
                        (i[r].q = i[r].q || []).push(arguments)
                    }, i[r].l = 1 * new Date();
                    a = s.createElement(o),
                            m = s.getElementsByTagName(o)[0];
                    a.async = 1;
                    a.src = g;
                    m.parentNode.insertBefore(a, m)
                })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
                ga('create', '{{ga_tracking}}', 'auto');
                ga('send', 'pageview');
            </script>
        {%endif%}
    {% endblock %}
    {% block view_javascripts %}
    {% endblock %}
    <script type="text/javascript" id="cookieinfo" src="//cookieinfoscript.com/js/cookieinfo.min.js" data-cookie="CookieInfoScript" data-text-align="center" data-linkmsg="Check our Privacy Policy." data-moreinfo="/privacy-policy" data-close-text="Got it!">
    </script>
</body>
</html>
